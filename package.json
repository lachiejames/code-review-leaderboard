{
    "name": "code-review-leaderboard",
    "version": "1.0.0",
    "description": "Find out who is doing the most code reviews at your organisation",
    "author": "Lachie James",
    "license": "MIT",
    "main": "lib/src/index.js",
    "dependencies": {
        "date-fns": "2.22.1",
        "gaxios": "4.3.0",
        "lodash": "4.17.21",
        "ora": "5.4.1",
        "prompts": "2.4.1",
        "table": "6.7.1",
        "url-regex": "5.0.0"
    },
    "devDependencies": {
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/commit-analyzer": "8.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/npm": "7.1.3",
        "@semantic-release/release-notes-generator": "9.0.3",
        "@types/jest": "26.0.23",
        "@types/lodash": "4.14.170",
        "@types/node": "15.0.2",
        "@types/prompts": "2.0.13",
        "@typescript-eslint/eslint-plugin": "4.28.0",
        "@typescript-eslint/parser": "4.28.0",
        "eslint": "7.28.0",
        "eslint-plugin-import": "2.23.4",
        "find-root": "1.1.0",
        "jest": "27.0.5",
        "jest-mock-console": "1.1.0",
        "nock": "13.1.0",
        "prettier": "2.3.1",
        "semantic-release": "17.4.4",
        "ts-jest": "27.0.3",
        "typescript": "4.3.4"
    },
    "scripts": {
        "build": "tsc",
        "format": "prettier --write . & eslint . --fix",
        "start": "node .",
        "test": "jest --coverage --detectOpenHandles --runInBand",
        "semantic-release": "semantic-release"
    },
    "bin": {
        "code-review-leaderboard": "./lib/src/cli.js"
    },
    "directories": {
        "lib": "lib"
    },
    "files": [
        "lib/**"
    ],
    "repository": "https://github.com/lachiejames/code-review-leaderboard.git",
    "keywords": [
        "leaderboard",
        "code-review",
        "pull-request"
    ],
    "bugs": {
        "url": "https://github.com/lachiejames/code-review-leaderboard/issues"
    },
    "homepage": "https://github.com/lachiejames/code-review-leaderboard#readme",
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/npm",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "CHANGELOG.md"
                    ]
                }
            ]
        ]
    }
}
