{
    "name": "code-review-leaderboard",
    "version": "1.0.1",
    "description": "Find out who is doing the most code reviews at your organisation",
    "author": "Lachie James",
    "license": "UNLICENSED",
    "main": "lib/src/index.js",
    "dependencies": {
        "date-fns": "2.21.3",
        "gaxios": "4.2.1",
        "ora": "5.4.0",
        "table": "6.7.1"
    },
    "devDependencies": {
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/commit-analyzer": "8.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/npm": "7.1.1",
        "@semantic-release/release-notes-generator": "9.0.2",
        "@types/jest": "26.0.23",
        "@types/node": "15.0.2",
        "@typescript-eslint/eslint-plugin": "4.23.0",
        "@typescript-eslint/parser": "4.23.0",
        "eslint": "7.23.0",
        "eslint-plugin-import": "2.22.1",
        "find-root": "1.1.0",
        "jest": "26.6.3",
        "jest-mock-console": "1.0.1",
        "nock": "13.0.11",
        "prettier": "2.3.0",
        "ts-jest": "26.5.6",
        "typescript": "4.2.4",
        "semantic-release": "17.4.2"
    },
    "scripts": {
        "build": "tsc",
        "format": "prettier --write . & eslint . --fix",
        "start": "yarn build & node .",
        "test": "jest --coverage --detectOpenHandles --runInBand",
        "semantic-release": "semantic-release"
    },
    "directories": {
        "lib": "lib"
    },
    "repository": "https://github.com/lachiejames/code-review-leaderboard.git",
    "keywords": [
        "leaderboard",
        "code-review",
        "pull-request"
    ],
    "bugs": {
        "url": "https://github.com/lachiejames/code-review-leaderboard/issues"
    },
    "homepage": "https://github.com/lachiejames/code-review-leaderboard#readme",
    "release": {
        "branches": [
            "main"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/npm",
            "@semantic-release/release-notes-generator",
            [
                "@semantic-release/changelog",
                {
                    "changelogFile": "CHANGELOG.md"
                }
            ],
            [
                "@semantic-release/git",
                {
                    "assets": [
                        "CHANGELOG.md"
                    ]
                }
            ]
        ]
    }
}
